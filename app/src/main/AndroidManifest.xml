<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dicoding.picodiploma.jfood_android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="jfood-android"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".PromoListActivity"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".ReviewHistoryActivity"
            android:parentActivityName=".HistoryInvoiceActivity" />
        <activity
            android:name=".HistoryInvoiceActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".CartActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".FoodDetailsActivity" />
        <activity android:name=".LoginLauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SelesaiPesananActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".BuatPesananActivity"
            android:parentActivityName=".CartActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
    </application>

</manifest>